extends layouts/_layout
block content
  block header
    include partials/_header
  .pricing
    .pricing__title
      .container
        h1 Pricing
      .pricing__select
        strong Select Currency
        ul
          li(data-one="199" data-two="299").active $USD
          li(data-one="349" data-two="529") A$ AUD
          li(data-one="175" data-two="249") £ GBP
          li(data-one="15000" data-two="22000") ₹ INR
    .container
      .pricing__tariff
        .row
          .col-lg-4.col-12
            .pricing__tariff__block.wow.flipInY(data-wow-duration='1.5s')
              .pricing__tariff__block__title
